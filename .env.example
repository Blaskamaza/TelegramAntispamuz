# UZ AI Factory â€” Environment Configuration
# Copy this to .env and fill in your values

# === API Keys ===
GOOGLE_API_KEY=your_gemini_api_key_here
VERTEX_PROJECT_ID=your_gcp_project_id
VERTEX_LOCATION=us-central1

# === V2 Rollout ===
# Canary deployment: Start with 10%, increase after testing
V2_ROLLOUT_PERCENTAGE=100

# Max parallel tasks running at once
V2_MAX_PARALLEL_TASKS=5

# === Auto-Discovery ===
# Max tasks created per auto-discovery run
AUTO_DISCOVERY_MAX_TASKS=5

# XP threshold for auto-merge
AUTO_MERGE_XP_THRESHOLD=80

# === MCP Server API Keys ===
BRAVE_API_KEY=your_brave_api_key_here
GITHUB_TOKEN=your_github_personal_access_token_here
DATABASE_URL=postgresql://user:password@host:5432/database

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-anon-key-here


# === HuggingFace Config ===
HF_TOKEN=your_huggingface_token_here

# === n8n Config ===
N8N_API_URL=http://localhost:5678

# Optional MCP Servers (future)
SLACK_BOT_TOKEN=your_slack_bot_token
SLACK_TEAM_ID=your_slack_team_id
EVERART_API_KEY=your_everart_key
